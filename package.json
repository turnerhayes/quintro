    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "cross-env": "5.0.0",
    "dotenv": "^5.0.1",
    "express": "^4.17.0",
    "express-session": "^1.16.1",
    "fontfaceobserver": "2.0.9",
    "history": "4.6.3",
    "hoist-non-react-statics": "2.1.1",
    "howler": "^2.1.2",
    "http-status-codes": "^1.3.2",
    "immutable": "3.8.1",
    "intl": "1.2.5",
    "invariant": "2.2.2",
