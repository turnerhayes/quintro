    "mongoose": "^5.12.7",
    "morgan": "^1.9.1",
    "normalize.css": "^7.0.0",
    "notifyjs": "^3.0.0",
    "passport": "^0.4.0",
    "passport-facebook-rwky": "^2.3.1",
    "passport-google-oauth20": "^1.0.0",
    "passport-twitter": "^1.0.4",
    "prop-types": "15.5.10",
    "proper-url-join": "^1.2.0",
    "qs": "^6.7.0",
    "react": "^16.8.6",
    "react-bem-helper": "^1.4.1",
    "react-dom": "^16.8.6",
    "react-helmet": "5.1.3",
    "react-hot-loader": "^4.8.8",
    "react-immutable-proptypes": "^2.1.0",
    "react-intl": "^2.9.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^6.0.1",
