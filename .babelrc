{
	"plugins": [
		"transform-undefined-to-void",
		"formatjs"
	],
	"presets": [
		[
			"@babel/preset-env",
			{
				"modules": "auto"
			}
		],
		"@babel/preset-react"
	],
	"env": {
		"production": {
			"only": [
				"app",
				"shared-lib"
			],
			"plugins": [
				"transform-react-remove-prop-types",
				"transform-react-constant-elements",
				"transform-react-inline-elements"
			]
		},
		"test": {
			"plugins": [
				"transform-es2015-modules-commonjs",
				"dynamic-import-node"
			]
		}
	}
}
